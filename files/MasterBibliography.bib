	Bdsk-File-1 = {}}

@article{abraham2000,
	abstract = {We have performed atomic simulations of crack
propagation along a weak interface
joining two harmonic crystals. The simulations show that a mode II shear
dominated crack can
accelerate to the Rayleigh wave speed and then nucleate an intersonic
daughter that travels at the
longitudinal wave speed. This contradicts the general belief that a
crack can travel no faster than
the Rayleigh speed.},
