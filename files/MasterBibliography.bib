	doi = {10.1029/2006JB004612},
	journal = {J. Geophys. Res.},
	number = {B9},
	title = {Effects of mesoscopic-scale fault structure on dynamic earthquake ruptures: Dynamic formation of geometrical complexity of earthquake faults},
