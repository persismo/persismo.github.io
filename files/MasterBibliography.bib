	journal = {IInt. J. Disaster Risk Reduc.},
	pages = {101229},
	title = {Post-tsunami survey of the 28 September 2018 tsunami near Palu Bay in Central Sulawesi, Indonesia: Impacts and challenges to coastal communities},
