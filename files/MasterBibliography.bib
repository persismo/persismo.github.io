@article{rice1999a,
	abstract = {Flash heating at frictional asperity contacts has been suggested in engineering tribology as the key to understanding the slip rate dependence of dry friction in metals at high rates (e.g., Bowden and Thomas, {\it Proc. Roy. Soc.}, 1954; Ettles, {\it J. Tribol.}, 1986; Lim and Ashby, {\it Acta Met.}, 1987; Molinari et al., {\it J. Tribol.}, 1999). The concept is developed here for fault friction during rapid, possibly large, slips. Let $T_{f}$ be the gradually evolving average temperature along a sliding fault zone, and $T$ be a local, highly transient, temperature at an asperity contact from flash heating during its brief lifetime $\theta=D/V$. Here $D$ is contact size and $V$ is slip rate. The contact shear strength $\tau_{c}$ must degrade continuously with increasing $T$. A simple yet informative model is to assume $\tau_{c}$ is constant with $T$ up to a weakening temperature $T_{w}$, and then negligible for $T$ $>$ $T_{w}$. Then there will be a thermal efect on friction only if the time $\theta_{w}$ necessary for shear heating to raise $T$ from $T_{f}$ to $T_{w}$ is less than the lifetime $\theta$. We can estimate $\theta_{w} = (\pi \alpha/V^{2}) [\rho c (T_{w}-T_{f})/\tau_{c}]^{2}$ by equating the heat input $\tau_{c} V \theta_{w}$ at the contact to the thermal energy storage $\rho c (T_{w}-T_{f})$ over an effective distance $(\pi \alpha \theta_{w})^{1/2}$, based on 1D heat conduction, where $\alpha$ is thermal diffusivity and $\rho c$ is the specific heat. The expression is relevant if $\theta_{w}$ $<$ $\theta$, which occurs for $V$ $>$ $V_{w} = (\pi \alpha/D) [\rho c (T_{w}-T_{f})/\tau_{c}]^{2}$, a characteristic weakening velocity. Taking $\alpha =$ 1 (mm)$^{2}$/s, $\rho c =$ 4 MJ/m$^{3}$K, $D =$ 5 $\mu$m, $T_{w}-T_{f} =$ 900$\deg$C $-$ 200$\deg$C = 700 K and $\tau_{c} =$ 0.02 to 0.10 {\it G}(shear modulus) = 0.6 to 3.0 GPa, this gives $V_{w} =$ 0.5 to 14 m/s for onset of severe thermal weakening. The lower limit of this $V_{w}$ range corresponds to $\tau_{c} =$ 0.10 {\it G}, of order of the theoretical shear strength. A similarly simple estimate of the effect on macroscopic friction is given by neglecting the statistics of $D$ and other parameters, and noting that when $V$ $>$ $V_{w}$, $(\tau_{c})_{avg} = \tau_{c} \theta_{w}/\theta (= \tau_{c} V_{w}/V)$ is the average shear stress borne by a contact. The macroscopic shear strength $\tau$ and normal stress $\sigma$ on the fault satisfy $\tau/\sigma = (\tau_{c})_{avg}/\sigma_{c} = f$, where $\sigma_{c}$ is the local normal stress at the contacts and $f$ is the friction coefficient. Thus, for $V$ $>$ $V_{w}$ the friction coefficient diminishes with rate as $f = f_{o} V_{w}/V$, where $f_{o} = \tau_{c}/\sigma_{c}$ is the coefficient at lower speeds. The macroscopic shear heating rate $\tau V$ drives the averge fault temperature $T_{f}$ above its initial value. That rate becomes independent of $V$ when $V$ $>$ $V_{w}$, because then $\tau V = f \sigma V = f_{o} \sigma V_{w}$. Further, since $V_{w}$ $\sim$ $(T_{w}-T_{f})^{2}$, the hotter the fault, the smaller this maximum heating rate. At the next level of sophistication, $\tau_{c}$ and $\sigma_{c}$, thus also $f_{o}$ and $D$, should not be regarded as constants, but rather as functions of $T_{f}$ and $V$, which dependences are partly documented in experimental studies at slow rates in the framework of rate/state friction (e.g., Blanpied et al., {\it JGR}, 1998). Also, the continual elevation of $T_{f}$ in rapid large slips may lead to macroscopic melt evolution and, in a fluid infiltrated fault zone, to fluid pressurization and effective stress reduction, at least if not alleviated by inelastic dilatancy or increased permeability},
	author = {Rice, J. R.},
	date-added = {2008-11-07 16:42:33 -0800},
	date-modified = {2016-12-12 14:01:55 +0000},
	journal = {Eos Trans. AGU},
	keywords = {conference},
	pages = {F6811},
	title = {Flash heating at asperity contacts and rate-dependent friction},
	volume = {80(46), Fall Meet. Suppl.},
	year = {1999}}

