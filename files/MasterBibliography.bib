	journal = {Science},
	month = {May},
	pages = {1337--1340},
	title = {Cracks faster than the shear wave speed},
	volume = {284},
	year = {1999},
	Bdsk-File-1 = {}}

@article{broberg1999b,
	abstract = {An analytic solution is given for intersonic mode II
crack acceleration. The
solution is obtained by first considering a self-similar problem, motion
of a semi-infinite crack at
constant velocity, under the action of a constant crack face load,
appearing behind the crack edge.
This solution is then repeatedly used in a superposition scheme, which
finally yields the solution
for an accelerating crack.},
