@article{dunham2003,
	abstract = {Seismic data indicate that fault ruptures follow
complicated paths with variable
velocity because of inhomogeneities in initial stress or fracture
energy. We report a phenomenon
unique to three-dimensional cracks: Locally stronger fault sections,
rather than slowing ruptures,
drive them forward at velocities exceeding the shear wave speed. This
supershear mechanism
differentiates barrier and asperity models of fault heterogeneity, which
previously have been
regarded as indistinguishable. High strength barriers concentrate
energy, producing potentially
destructive pulses of strong ground motion.},
	author = {Dunham, E. M. and Favreau, P. and Carlson, J. M.},
