@article{morton2012,
	abstract = {During an earthquake, fracture damage can be imparted into the wallrock surrounding faults due to stress pulses induced by the radiation of seismic waves. High strain rates restricted to small strains in fast propagating earthquakes can induce pervasive microscopic fracturing which, if fast enough, can pulverize the rock. Therefore, large earthquake ruptures can impart significant changes on the strength, elastic and fluid flow properties of a fault zone immediately after rupture propagation, and prior to fault slip. Variations in hydraulic properties can therefore exert both positive and negative feedbacks on processes controlling fault slip depending on the type of damage imparted at the rupture tip. However, to date the extent of such coseismic high-strain rate damage and the effect on hydraulic properties is not fully understood. Here we show that damage due to rupture-induced high strain rate pervasive microfracturing can enhance bulk wallrock permeability by up to 7 orders of magnitude in a single high strain-rate event, with the highest strain rates that lead to pulverization reducing the permeability by the same order. We show that laboratory measurements of permeability on granitic samples deformed at high strain rates compare well with measurements of permeability in natural samples collected from the damage zone of San Jacinto fault. Calculations of strain rates expected surrounding a dynamic shear fracture propagating with a velocity approaching the limiting Rayleigh wave speed show that high strain rates attained during shock experiments are attainable in nature. Our results show that the size of a rupture can lead to significant variations in off-fault permeability, directly effecting fault weakening processes such as thermal pressurization, and can act as a toggle switch to either promote or arrest faul},
	author = {Morton, N and Mitchell, T M and Doan, M. L. and Girty, Gary and Rockwell, T. K. and Renner, J.},
	date-added = {2013-06-10 15:46:55 +0000},
	date-modified = {2013-06-10 17:07:04 +0000},
	journal = {2012 Fall Meeting, AGU, San Francisco, Calif., 3-7 Dec.},
	title = {Ultra-high permeability induced by seismic shockwaves near the San Jacinto Fault},
	year = {2012}}

